class Solution:
    def constructTransformedArray(self, nums: List[int]) -> List[int]:
        res=[]
        for i in range(len(nums)):
            if(nums[i]>0):
                target=(i+nums[i])%len(nums)
                print(target)
                res.append(nums[target])
            elif(nums[i]<0):
                target=((i+nums[i])%len(nums)+len(nums))%len(nums)
                res.append(nums[target])
            else:
                res.append(nums[i])
        return res
        
