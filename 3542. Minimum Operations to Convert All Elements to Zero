    # def minOperations(self, nums: List[int]) -> int:
    #     count=0 
    #     while(nums.count(0)!=len(nums)):
    #         minind1=nums.index(self.findmin(nums))
    #         minind2=self.minfromreverseind(nums,nums[minind1],minind1)
    #         minfound=nums[minind1]
