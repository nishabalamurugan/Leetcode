    def minOperations(self, nums: List[int]) -> int:
        stack=[0]*(len(nums)+1)
        top=0
        count=0 
        for i in range(len(nums)):
            while(stack[top]>nums[i]):
                top-=1
                count+=1 
            if(stack[top]!=nums[i]):
                top+=1
                stack[top]=nums[i]

        return count+top
