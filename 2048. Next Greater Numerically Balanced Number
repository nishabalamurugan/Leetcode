class Solution:
    def nextBeautifulNumber(self, n: int) -> int:
        if(n==0):
            return 1
        for i in range(n+1,n*100):
            f=0
            if('0' not in str(i)):
                for j in range(1,10):
                    count=str(i).count(str(j))
                    if(count!=int(j) and count>0):
                        f=1
                        break 
                if(f==0):
                    return i 
        return 0
                

        
