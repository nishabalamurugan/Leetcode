class Solution:
    def minBitwiseArray(self, nums: List[int]) -> List[int]:
        res=[]
        for i in nums:
            found=False
            for j in range(0,i+1):
                if(j|(j+1)==i):
                    res.append(j)
                    found=True
                    break;
            if not found:
                res.append(-1)
        return res
        
