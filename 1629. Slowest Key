class Solution:
    def slowestKey(self, releaseTimes: List[int], keysPressed: str) -> str:
        res=""
        releaseTimes.insert(0,0)
        maxi=0
        for i in range(1,len(releaseTimes)):
            if(releaseTimes[i]-releaseTimes[i-1]>maxi):
                maxi=releaseTimes[i]-releaseTimes[i-1]
                if(res):
                    # if(res<keysPressed[i-1]):
                    #     res=""
                    #     res+=keysPressed[i-1]
                    res=""
                    res+=keysPressed[i-1]
                else:
                    res+=keysPressed[i-1]
            elif(releaseTimes[i]-releaseTimes[i-1]==maxi):
                if(res and res<keysPressed[i-1]):
                    res=""
                    res+=keysPressed[i-1]
        # return res,maxi
        return res

        
