class Solution:
    def isOneBitCharacter(self, bits: List[int]) -> bool:
        count=0
        for i in range(len(bits)-1):
            if(bits[i]==1):
                count+=1 
                if(count<2):
                    continue 
                elif(count==2):
                    count=0
                else:
                    return False 
            else:
                count=0 
        if(count==1):
            return False
        return True

        
