class Solution:
    def minTimeToVisitAllPoints(self, points: List[List[int]]) -> int:
        dist=0 
        for i in range(1,len(points)):
            x_dist=abs(points[i][0]-points[i-1][0])
            y_dist=abs(points[i][1]-points[i-1][1])
            print(x_dist,y_dist)
            dist+=max(x_dist,y_dist)
        return dist
        
